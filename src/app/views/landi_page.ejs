            Swal.fire({
                title:'<%=alertTitle%>',
                text:'<%=alertMessage%>',
                icon:'<%=alertIcon%>',
                ShowConfirmButton:'<%=showConfirmButton%>',
                timer:<%= timer %>
            }).then(()=>{
                window.location='/<%=ruta%>'
            })
        </script>  
        <% } %> 		
